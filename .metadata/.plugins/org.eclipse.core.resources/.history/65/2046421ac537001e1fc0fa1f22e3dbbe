package new_sambank;

public class prueba_metodos {
	public static void main(String[] args) {
		Cuenta miCuenta = new Cuenta();
		miCuenta.saldo = 300;
	    miCuenta.deposito(200);
	    
	    System.out.println(miCuenta.saldo);
	    
	    miCuenta.retiro(100);
	    System.out.println(miCuenta.saldo);
	    
	    cuenta CuentaJim = new Cuenta();
	    
	}

}
